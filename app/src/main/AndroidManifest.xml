<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loleventspma">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rabon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/rabon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SinglePlayerActivity" />
        <activity android:name=".Activities.PlayerActivity" />
        <activity android:name=".Activities.SingleTeamActivity" />
        <activity android:name=".Activities.Dashboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.LeagueActivity" />
        <activity android:name=".Activities.SingleLeagueActivity" />
    </application>

</manifest>